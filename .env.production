# Production Environment Variables

# AI Configuration
GOOGLE_API_KEY=sk-xxxxxxxxxxxx
STRIPE_SECRET_KEY=sk_live_xxxxxxxxxxxx
STRIPE_PUBLISHABLE_KEY=pk_live_xxxxxxxxxxxx
BACKEND_PORT=8000
FRONTEND_URL=https://aiagent.railway.app
